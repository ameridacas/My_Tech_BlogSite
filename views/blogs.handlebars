<div>
  <main class="container container-fluid mt-5">
    <h2>Your Blogs</h2>
    
    {{#each blogs}}
      <div class="blog-entry">
        <h3>{{this.title}}</h3>
        <p>{{this.description}}</p>
        <p>Date Created: {{this.date_created}}</p>
      </div>
    {{/each}}

    <div class="mt-3">
      <a href="/">Go to Home</a> |
      <a href="/logout">Logout</a>
    </div>
  </main>
</div>


